{
  "name": "compositly",
  "license": "UNLICENSED",
  "description": "This is a LIB-react for the Compositly project. It is a React component library",
  "version": "0.0.1",
  "author": {
    "name": "Compositly",
    "url": "https://compositly.com"
  },
  "type": "module",
  "main": "./dist/bundle.umd.js",
  "module": "./dist/bundle.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bundle.es.js",
      "require": "./dist/bundle.umd.js"
    },
    "./dist/bundle.css": {
      "import": "./dist/bundle.css",
      "require": "./dist/bundle.css"
    },
    "./public/output.css": {
      "import": "./public/output.css",
      "require": "./public/output.css"
    },
    "./node_modules/react": {
      "import": "./node_modules/react/index.js",
      "require": "./node_modules/react/index.js"
    }
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "build": "microbundle-crl --globals react=React --jsxFragment React.Fragment --jsx React.createElement --css-modules true -i src/main.js -o dist/bundle.js",
    "dev": "microbundle-crl --globals react=React --jsxFragment React.Fragment --jsx React.createElement --css-modules true -i src/main.js -o dist/bundle.js watch",
    "css": "npx tailwindcss -i \"src/assets/css/input.css\" -o \"public/output.css\"",
    "css:dev": "npx tailwindcss -i \"./src/assets/css/input.css\" -o \"public/output.css\" --watch",
    "test": "jest --collectCoverage --coverageDirectory=\".coverage/\" --ci --reporters=default --reporters=jest-junit --watchAll=false --detectOpenHandles --forceExit",
    "lint": "eslint \"src/**/*.{js,jsx}\" \".storybook/stories/**/*.{js,jsx}\" \"__tests__/**/*test*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,}\" \".storybook/stories/**/*.{js,jsx}\" \"__tests__/**/*test*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,}\" \".storybook/stories/**/*.{js,jsx}\" \"__tests__/**/*test*.{js,jsx}\" --config \".prettier.json\"",
    "compile:to:landing": "npx cp compositly-*.tgz ../landing/libs/",
    "clean": "rm -rf dist dist-stories dist-docs .coverage package-lock.json junit.xml *.tgz yarn.lock node_modules",
    "ci": "yarn clean && yarn && yarn lint && yarn format && yarn test && yarn build && yarn pack",
    "ci:css": "yarn css && yarn lint && yarn format && yarn test && yarn build && yarn pack",
    "ci:compile": "yarn ci && yarn compile:to:landing",
    "stories": "storybook dev -p 6006",
    "stories:build": "storybook build -o dist-stories",
    "docs": "storybook dev --docs",
    "docs:build": "storybook build --docs -o dist-docs"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "-": "^0.0.1",
    "@headlessui/react": "^2.0.3",
    "@headlessui/tailwindcss": "^0.2.0",
    "@heroicons/react": "^2.1.3",
    "@radix-ui/react-slider": "^1.1.2",
    "animate.css": "^4.1.1",
    "dayjs": "^1.11.11",
    "framer-motion": "^11.2.6",
    "google-libphonenumber": "^3.2.34",
    "react": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-international-phone": "^4.3.0",
    "react-loader-spinner": "^6.1.6",
    "react-spinners": "^0.13.8",
    "react-use": "^17.5.0",
    "tailwind-children": "^0.5.0",
    "vite": "^5.2.0",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-unicode-property-regex": "^7.24.7",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@chromatic-com/storybook": "^1",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "daisyui": "^3.9.4",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-svg-transformer": "^1.0.0",
    "microbundle-crl": "^0.13.11",
    "npx": "^10.2.2",
    "postcss": "^8.4.32",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "storybook": "^8.0.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-children": "^2.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "^.+\\.svg$": "jest-svg-transformer",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "^uuid$": "<rootDir>/node_modules/uuid/dist/index.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "coverageReporters": [
      "html",
      "text",
      "text-summary",
      "cobertura"
    ]
  }
}
